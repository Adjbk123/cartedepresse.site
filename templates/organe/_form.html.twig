{{ form_start(form) }}
<div class="row">
    <div class="col-md-6">
        {{ form_row(form.designation) }}
    </div>
    <div class="col-md-6">
        {{ form_row(form.typeOrgane) }}
    </div>
</div>
<div class="row">
    <div class="col-md-6">
        {{ form_row(form.adresse) }}
    </div>
    <div class="col-md-6">
        {{ form_row(form.contact) }}
    </div>
</div>
<div class="row">
    <div class="col-md-6">
        {{ form_row(form.latitude) }}
    </div>
    <div class="col-md-6">
        {{ form_row(form.longitude) }}
    </div>
</div>
    {{ form_widget(form) }}
    <button class="btn btn-success">{{ button_label|default('Enregistrer') }}</button>
{{ form_end(form) }}
